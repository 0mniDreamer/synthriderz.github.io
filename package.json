{
  "name": "synthriderz.github.io",
  "version": "0.8.0",
  "description": "Open-source documentation for the Synth Riders Community Wiki!",
  "main": "update-assets.js",
  "scripts": {
    "serve": "docsify serve docs",
    "update-assets": "node update-assets.js",
    "format-code": "prettier --write .",
    "lint-code": "eslint .",
    "clean-assets": "rm -rf docs/assets/js docs/assets/css",
    "update-deps": "ncu -u && npm install"
  },
  "keywords": [
    "docsify",
    "synthriders",
    "wiki",
    "documentation"
  ],
  "author": "The Synth Riders Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/synthriderz/synthriderz.github.io"
  },
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "axios": "^1.8.3",
    "docsify": "^4.13.1",
    "docsify-cli": "^4.4.4",
    "docsify-copy-code": "^3.0.0",
    "docsify-count": "^1.1.0",
    "docsify-pagination": "^2.10.1",
    "docsify-plugin-flexible-alerts": "^1.1.1",
    "docsify-plugin-toc": "^1.3.2",
    "docsify-themeable": "^0.9.0",
    "docsify-top-banner-plugin": "^0.4.0",
    "docsify-updated": "^1.0.5",
    "fs-extra": "^11.3.0",
    "prismjs": "^1.30.0"
  },
  "devDependencies": {
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "npm-check-updates": "^17.1.15"
  }
}
